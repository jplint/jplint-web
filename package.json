{
  "name": "jplint-web",
  "version": "0.1.0",
  "description": "Linting Web App for JP Engineers.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jplint/jplint-web.git"
  },
  "keywords": [
    "lint",
    "typescript",
    "vue.js"
  ],
  "author": "sforzando LLC. and Inc.",
