    "@typescript-eslint/parser": "^3.9.0",
    "@vue/cli-plugin-babel": "~4.5.3",
    "@vue/cli-plugin-e2e-cypress": "~4.5.3",
    "@vue/cli-plugin-eslint": "~4.5.3",
    "@vue/cli-plugin-pwa": "~4.5.3",
    "@vue/cli-plugin-typescript": "~4.5.3",
